                      <li><a class="dropdown-item" href="{{path('app_top.new')}}">Nouveau</a></li>
                    </ul>
                  </li>

                  <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="{{path('app_direction')}}" role="button" data-bs-toggle="dropdown" aria-expanded="false">
